{
  "kind": "collectionType",
  "collectionName": "processes",
  "info": {
    "name": "Progress"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "time": {
      "type": "biginteger"
    },
    "promotion_title": {
      "type": "text"
    },
    "subtask_content": {
      "type": "text"
    },
    "code": {
      "type": "integer"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "confirmed",
        "unconfirmed"
      ]
    },
    "restaurant": {
      "model": "restaurant",
      "via": "progresses"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "progresses"
    }
  }
}

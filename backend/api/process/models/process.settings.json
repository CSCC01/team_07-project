{
  "kind": "collectionType",
  "collectionName": "processes",
  "info": {
    "name": "Progress"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "processes"
    },
    "promotion": {
      "model": "promotion",
      "via": "progresses"
    },
    "subtasks": {
      "via": "progress",
      "collection": "subtask"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "ongoing",
        "completed"
      ]
    }
  }
}

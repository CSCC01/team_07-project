{
  "kind": "collectionType",
  "collectionName": "requests",
  "info": {
    "name": "Request"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "subtask": {
      "model": "subtask",
      "via": "requests"
    },
    "restaurant": {
      "model": "restaurant",
      "via": "requests"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "rejected"
      ]
    },
    "coupon": {
      "model": "coupon",
      "via": "requests"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "subtask",
        "coupon"
      ]
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "requests"
    }
  }
}
{
  "kind": "collectionType",
  "collectionName": "subtasks",
  "info": {
    "name": "Subtask"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "description": {
      "type": "richtext"
    },
    "progress": {
      "model": "process",
      "via": "subtasks"
    },
    "requests": {
      "collection": "request",
      "via": "subtask"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "ongoing",
        "completed"
      ]
    },
    "index": {
      "type": "integer"
    }
  }
}
